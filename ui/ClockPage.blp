using Gtk 4.0;
using Adw 1;

template $ClockPage : Adw.PreferencesPage {
    title: "Clock";
    icon-name: "x-office-calendar-symbolic";
    description: "Clock settings";

    Adw.PreferencesGroup {
        Adw.EntryRow dateTimeFormat {
            title: "Format string";

            [suffix]
            Gtk.Label previewLabel {}
        }
    }
}
